<com.cloudbees.jenkins.plugins.bitbucket.BitbucketSCMSource>
    <id>com.cloudbees.jenkins.plugins.bitbucket.BitbucketSCMNavigator::https://bitbucket.org::jenkinsci::bitbucket-scm-filter-aged-refs</id>
    <credentialsId>bitbucket-cloud</credentialsId>
    <checkoutCredentialsId>SAME</checkoutCredentialsId>
    <repoOwner>cloudbeers</repoOwner>
    <repository>stunning-adventure</repository>
    <includes>*</includes>
    <excludes></excludes>
    <autoRegisterHook>false</autoRegisterHook>
    <sshPort>-1</sshPort>
    <repositoryType>GIT</repositoryType>
    <traits>
        <org.jenkinsci.plugins.scm_filter.BitbucketAgedRefsTrait plugin="bitbucket-scm-filter-aged-refs@${project.version}">
            <branchRetentionDays>30</branchRetentionDays>
            <prRetentionDays>40</prRetentionDays>
            <tagRetentionDays>50</tagRetentionDays>
            <branchExcludeFilter>main hotfix-*</branchExcludeFilter>
        </org.jenkinsci.plugins.scm_filter.BitbucketAgedRefsTrait>
    </traits>
</com.cloudbees.jenkins.plugins.bitbucket.BitbucketSCMSource>